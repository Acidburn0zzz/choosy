[{
    "name" : "Email",
    "key" : "email",
    "parameters" :
    [{
        "name" : "Body Text",
        "description" : "Text to insert into the body of a new email",
        "key" : "body"
    },
    {
        "name" : "Recipients - To",
        "description" : "Recipient(s) email address(ses)",
        "key" : "to"
    },
    {
        "name" : "Recipients - CC",
        "description" : "Email addresses to CC (copy)",
        "key" : "cc"
    },
    {
        "name" : "Subject",
        "description" : "Email subject",
        "key" : "subject"
    }],
    "actions" :
    [{
        "name" : "Compose",
        "key" : "compose"
     }],
    "apps" :
    [{
        "name" : "Mail",
        "key" : "mail",
        "app_url_scheme" : "mailto:",
